{
  "name": "@nestjs-toolkit/graphql-mercurius",
  "version": "2.0.3",
  "description": "NestJS Toolkit - GraphQL Mercurius",
  "author": "Valmir Barbosa",
  "license": "MIT",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.build.json && yarn copy",
    "copy": "cp package.json README.md LICENSE dist && mkdir -p dist/schemas && cp lib/**/*.graphql dist/schemas",
    "prepare": "",
    "format": "prettier --write \"{lib,tests}/**/*.ts\"",
    "lint": "eslint \"{lib,tests}/**/*.ts\" --fix",
    "test": "jest --config tests/jest.json",
    "test:watch": "jest --config tests/jest.json --watch",
    "test:cov": "jest --config tests/jest.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config tests/jest.json --runInBand",
    "publish:prod": "cd dist && npm publish --access public",
    "publish:local": "cd dist && npm publish --registry http://localhost:4873"
  },
  "peerDependencies": {
    "@nestjs-toolkit/base": "^2.0.1",
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0"
  },
  "devDependencies": {
    "@nestjs-toolkit/base": "^2.0.3",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.5",
    "@nestjs/core": "^10.4.5",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.5",
    "@types/jest": "29.5.13",
    "@types/node": "18.19.56",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bson": "^5.5.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.7.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "string-format": "^2.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "yarn@3.5.1"
}
